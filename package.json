{
  "name": "@gala-chain/decentralized-ed",
  "version": "1.1.8",
  "description": "Product Chaincode",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "chaincode:start": "fabric-chaincode-node server",
    "chaincode:start:dev": "nodemon --watch lib --watch .env -r dotenv/config ./node_modules/.bin/fabric-chaincode-node server",
    "chaincode:dev": "npm run build && concurrently 'npm run build:w' 'npm run chaincode:start:dev'",
    "start": "node lib/src/cli.js start",
    "clean": "tsc -b --clean",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w",
    "lint": "eslint . --ext .ts,.js",
    "fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' 'e2e/**/*.ts' --write",
    "prepublishOnly": "npm run format && npm run build && npm run lint && npm run test",
    "network:start": "galachain network:up -C=product-channel -t=curator -n=basic-product -d=./ --envConfig=./.dev-env --watch",
    "network:up": "galachain network:up -C=product-channel -t=curator -n=basic-product -d=./ --envConfig=./.dev-env --contracts=$(node lib/src/cli.js get-contract-names | tail -n 1)",
    "network:prune": "galachain network:prune",
    "network:recreate": "npm run network:prune && rm -rf ./test-network && npm run network:start",
    "test": "jest",
    "test:e2e": "jest --config=e2e/jest.config.js --runInBand",
    "update-snapshot": "jest --updateSnapshot"
  },
  "dependencies": {
    "@gala-chain/api": "1.1.8",
    "@gala-chain/chaincode": "1.1.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.8",
    "better-sqlite3": "^9.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "fabric-contract-api": "2.4.2",
    "fabric-shim": "2.4.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@gala-chain/cli": "1.1.8",
    "@gala-chain/client": "1.1.8",
    "@gala-chain/test": "1.1.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "axios": "^0.27.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-prettier-config": "^1.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
